let g:loaded_matchparen=1
()
set autochdir
set autoindent
set background=dark
set clipboard=unnamedplus
set expandtab
set hidden
set hlsearch
set ignorecase
set incsearch
set mouse=a
set noswapfile
set number
set smartcase
set smartindent
set tabstop=2
set wildmode=list:full

map <Leader>, :edit ~/.vimrc<CR>
map <Leader><lt> :source ~/.vimrc<CR>
